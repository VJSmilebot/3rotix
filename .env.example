# Supabase Configuration
# Get these values from your Supabase project dashboard
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Livepeer Configuration
# Get this from your Livepeer Studio dashboard
LIVEPEER_API_KEY=your_livepeer_api_key

# Access Control Keys for JWT-gated content
# Generate these using crypto libraries
ACCESS_CONTROL_PRIVATE_KEY=your_base64_encoded_private_key
NEXT_PUBLIC_ACCESS_CONTROL_PUBLIC_KEY=your_base64_encoded_public_key

# Next.js Configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret

# Add other environment-specific variables here
NODE_ENV=development